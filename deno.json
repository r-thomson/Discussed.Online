{
	"$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
	"imports": {
		"@hono/ua-blocker": "npm:@hono/ua-blocker@0.1.14",
		"@sentry/deno": "npm:@sentry/deno@10.32.1",
		"@std/assert": "jsr:@std/assert@1.0.15",
		"@std/async": "jsr:@std/async@1.0.15",
		"@std/encoding": "jsr:@std/encoding@1.0.10",
		"@std/fs": "jsr:@std/fs@1.0.19",
		"@std/path": "jsr:@std/path@1.1.2",
		"@std/testing": "jsr:@std/testing@1.0.16",
		"autoprefixer": "npm:autoprefixer@10.4.21",
		"date-fns": "npm:date-fns@4.1.0",
		"hono": "npm:hono@4.10.3",
		"postcss": "npm:postcss@8.5.6",
		"redis": "npm:redis@4.7.0",
		"tailwindcss": "npm:tailwindcss@3.4.10"
	},
	"tasks": {
		"start": "deno run --permission-set --env --watch --preload instrumentation.ts main.ts",
		"test": "deno test src/"
	},
	"lint": {
		"rules": {
			"exclude": ["no-explicit-any"]
		}
	},
	"fmt": {
		"useTabs": true,
		"indentWidth": 4,
		"singleQuote": true
	},
	"compilerOptions": {
		"jsx": "precompile",
		"jsxImportSource": "hono/jsx"
	},
	"permissions": {
		"default": {
			"env": true,
			"read": true,
			"write": true,
			"net": true,
			"sys": true
		}
	}
}
